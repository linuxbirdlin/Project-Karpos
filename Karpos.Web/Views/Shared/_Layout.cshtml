<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>Karpos - Paul's Backoffice</title>
    <meta content='width=device-width, initial-scale=1, maximum-scale=1' name='viewport'/>
    <link rel="shortcut icon"href="/content/img/favicon.ico" />
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="/content/https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="/content/https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
    <![endif]-->
    <!-- global css -->
    <link type="text/css" href="/content/css/app.css" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="/content/css/custom.css">
    <!-- end of global css -->
</head>
<body class="skin-coreplus">
    <div class="preloader">
        <div class="loader_img"><img src="/content/img/loader.gif" alt="loading..." height="64" width="64"></div>
    </div>
    <!-- header logo: style can be found in header-->
    <header class="header">
        <nav class="navbar navbar-static-top">
            <a href="index.html" class="logo">
                <!-- Add the class icon to your logo image or logo icon to add the margining -->
                <img src="/content/img/logo.png" alt="logo" />
            </a>
            <!-- Header Navbar: style can be found in header-->
            <!-- Sidebar toggle button-->
            <!-- Sidebar toggle button-->
            <div>
                <a href="#" class="navbar-btn sidebar-toggle" data-toggle="offcanvas" role="button">
                    <i class="fa fa-fw fa-bars"></i>
                </a>
            </div>
            <div class="navbar-right">
                <ul class="nav navbar-nav">
                   
                    <!-- User Account: style can be found in dropdown-->
                    <li class="dropdown user user-menu">
                        <a href="#" class="dropdown-toggle padding-user" data-toggle="dropdown">
                            <div class="riot">
                                <div>
                                    @ViewBag.Contact.Name
                                    <span>
                                        <i class="caret"></i>
                                    </span>
                                </div>
                            </div>
                        </a>
                        <ul class="dropdown-menu">
                            <!-- Menu Body -->
                            <li class="p-t-3">
                                <a href="user_profile.html"> <i class="fa fa-fw fa-user"></i> My Profile </a>
                            </li>
                            <li role="presentation"></li>
                            <li><a href="edit_user.html"> <i class="fa fa-fw fa-gear"></i> Account Settings </a></li>
                            <li role="presentation" class="divider"></li>
                            <!-- Menu Footer-->
                            <li class="user-footer">
                                <div class="pull-left">
                                    <a href="lockscreen.html">
                                        <i class="fa fa-fw fa-lock"></i>
                                        Lock
                                    </a>
                                </div>
                                <div class="pull-right">
                                    <a href="/Login/logout">
                                        <i class="fa fa-fw fa-sign-out"></i>
                                        Logout
                                    </a>
                                </div>
                            </li>
                        </ul>
                    </li>
                </ul>
            </div>
        </nav>
    </header>
    <div class="wrapper row-offcanvas row-offcanvas-left">
        <!-- Left side column. contains the logo and sidebar -->
        <aside class="left-side sidebar-offcanvas">
            <!-- sidebar: style can be found in sidebar-->
            <section class="sidebar">
                <div id="menu" role="navigation">
                    
                    <ul class="navigation">
                        <li>
                            <a href="/Request">
                                <i class="menu-icon fa fa-fw fa-home"></i>
                                <span class="mm-text ">Request</span>
                            </a>
                        </li>
                        @*<li>
                            <a href="index2.html">
                                <i class="menu-icon fa fa-fw fa-tachometer"></i>
                                <span class="mm-text ">Dashboard V2</span>
                            </a>
                        </li>*@
                       
                    </ul>
                    <!-- / .navigation -->
                </div>
                <!-- menu -->
            </section>
            <!-- /.sidebar -->
        </aside>
        <aside class="right-side">
            <!-- /content Header (Page header) -->
            <section class="content-header">
                <h1>Blank</h1>
                <ol class="breadcrumb">
                    <li>
                        <a href="index.html">
                            <i class="fa fa-fw fa-home"></i> Dashboard
                        </a>
                    </li>
                    <li> Pages</li>
                    <li class="active">
                        <a href="blank.html">Blank</a>
                    </li>
                </ol>
            </section>
            <!-- Main /content -->
            <section class="content">
               @RenderBody()
            </section>
            <!-- /./content -->
        </aside>
        <!-- /.right-side -->
    </div>
    <!-- /.right-side -->
    <!-- ./wrapper -->

    
    <!-- global js -->
    <script src="/content/js/app.js" type="text/javascript"></script>
    <!-- end of page level js -->
</body>
</html>
